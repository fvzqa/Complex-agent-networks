  {
   "cell_type": "code",
   "execution_count": 137,
   "metadata": {},
   "outputs": [],
   "source": [
    "def NetShield_plus(A,k,b):\n",
    "    w1, v1 = LA.eig(A)\n",
    "    lam = w1[0] \n",
    "    u= v1[:,0]\n",
    "    S_1=[]\n",
    "    t = math.floor(k/b)\n",
    "    \n",
    "    for j in range(t-1):\n",
    "        S_1 = NetShield(A,b)\n",
    "        S.extend(S_1)\n",
    "        for g in S_1:\n",
    "            A =np.delete(A,g,axis=0)\n",
    "            A =np.delete(A,g,axis=1)\n",
    "    \n",
    "    if k > t*b:\n",
    "        S_1 = NetShield(A,k-t*b)\n",
    "        S.extend(S_1)\n",
    "    \n",
    "    return(S)    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 138,
   "metadata": {},
   "outputs": [],
   "source": [
    "A = nx.adjacency_matrix(G)\n",
    "A = A.todense()\n",
    "w1, v1 = LA.eig(A)\n",
    "lam = w1[0] \n",
    "u=v1[:,0]\n",
    "S = []"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 51,
   "metadata": {},
   "outputs": [],
